@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "SubMaster.cshtml";
}

<div class="container">
	@if (CurrentPage.HasValue("memberPhoto"))
	{
		<img class="boardProfilePhoto" src='@CurrentPage.GetCropUrl("memberPhoto", "memberProfile")'/>
	}

	<h1>@Umbraco.Field("pageName")</h1>
	<h2>@Umbraco.Field("title")</h2>
						


	@Umbraco.Field("memberDetails")
</div>